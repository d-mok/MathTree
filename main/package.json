{
  "name": "mathtree",
  "version": "1.0.0",
  "scripts": {
    "publish": "npm run build-edge && npm run build-type",
    "build-edge": "esbuild src/index.ts --bundle --outfile=../dist/edge/mathtree.js",
    "build-type": "npx tsc --project tsconfig.d.json",
    "build-stable": "esbuild src/index.ts --bundle --minify --target=es6 --outfile=../dist/stable/mathtree.js",
    "jest": "cross-env NODE_OPTIONS=--experimental-vm-modules jest ./src/Math",
    "jest_soil": "jest ./src/Soil -debug",
    "dev": "esbuild src/index.ts --bundle --outfile=../dist/dev/mathtree.js"
  },
  "devDependencies": {
    "esbuild": "^0.14.7"
  },
  "dependencies": {
    "contract": "file:../packages/contract",
    "fate": "file:../packages/fate",
    "gauss": "file:../packages/gauss",
    "katex": "^0.13.23",
    "paint": "file:../packages/paint",
    "ruby": "file:../packages/ruby"
  }
}
